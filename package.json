    "babel-preset-es2015": "^6.24.1",
    "daggy": "^1.2.0",
    "documentation": "^6.3.3",
    "eslint": "^4.19.1",
    "flow-bin": "^0.72.0",
    "jsverify": "^0.8.3",
    "ramda": "^0.25.0",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-flow": "^1.1.1",
