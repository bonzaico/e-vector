  "description": "A 2D or 3D Euclidean vector library for Node.js and the browser.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index/.umd.min.js",
  "files": [
    "src",
    "dist/*.js"
  ],
  "scripts": {
